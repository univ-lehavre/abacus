{
  "name": "@univ-lehavre/openalex",
  "version": "1.0.0-alpha",
  "description": "Fouille des donn√©es OpenAlex",
  "author": "Pierre-Olivier Chasset",
  "license": "UNLICENSED",
  "scripts": {
    "build": "preconstruct build",
    "dev": "preconstruct watch",
    "clean": "rm -fR dist",
    "types:check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,js}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "test": "jest",
    "start": "run-s build start:only",
    "start:only": "node ./dist/index.js",
    "demo": "pnpm -s tsx demo/index.ts",
    "publish:only": "npm publish --access public",
    "publish:check": "npm pack --dry-run",
    "publish": "run-s build publish:only"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/qs": "^6.14.0",
    "@types/yargs": "^17.0.33",
    "@univ-lehavre/typescript-config": "workspace:^"
  },
  "dependencies": {
    "@clack/prompts": "1.0.0-alpha.4",
    "@duckdb/node-api": "1.3.4-alpha.27",
    "@univ-lehavre/strings-clustering": "workspace:^",
    "@xenova/transformers": "^2.17.2",
    "effect": "^3.17.13",
    "lodash": "^4.17.21",
    "picocolors": "^1.1.1",
    "qs": "^6.14.0",
    "uuid": "^13.0.0",
    "yargs": "^18.0.0"
  },
  "main": "dist/univ-lehavre-openalex.cjs.js",
  "module": "dist/univ-lehavre-openalex.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/univ-lehavre-openalex.cjs.mjs",
        "default": "./dist/univ-lehavre-openalex.cjs.js"
      },
      "module": "./dist/univ-lehavre-openalex.esm.js",
      "import": "./dist/univ-lehavre-openalex.cjs.mjs",
      "default": "./dist/univ-lehavre-openalex.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "babel": {
    "targets": {
      "node": "22.19"
    },
    "presets": [
      "@babel/preset-env"
    ],
    "overrides": [
      {
        "test": "**/*.ts",
        "presets": [
          "@babel/preset-typescript"
        ]
      }
    ]
  }
}
